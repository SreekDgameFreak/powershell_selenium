    <tags>Selenium Wrapper Web Automation</tags>
    <dependencies>
        <dependency id="Selenium.WebDriver" version="3.5.2"/>
        <dependency id="Selenium.Support" version="3.5.2"/>
    </dependencies>
  </metadata>
  <files>
      <file src="bin/Release/NSelene.dll" target="lib" />
  </files>
</package>